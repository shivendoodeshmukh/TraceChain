{
  "name": "fablo-test-network-distributor",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Distributor",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Distributor": {
      "mspid": "DistributorMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/distributor.example.com/users/Admin@distributor.example.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.distributor.example.com",
        "peer1.distributor.example.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/distributor.example.com/users/Admin@distributor.example.com/msp/signcerts/Admin@distributor.example.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.distributor.example.com": {
      "url": "grpcs://peer0.distributor.example.com:7081",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/distributor.example.com/msp/tlscacerts/tlsca.distributor.example.com-cert.pem"
      }
    },
    "peer1.distributor.example.com": {
      "url": "grpcs://peer1.distributor.example.com:7082",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/distributor.example.com/msp/tlscacerts/tlsca.distributor.example.com-cert.pem"
      }
    }
  },
  "channels": {
    "distribution": {
      "peers": {}
    }
  }
}
