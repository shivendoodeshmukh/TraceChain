{
  "name": "fablo-test-network-supplier",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Supplier",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Supplier": {
      "mspid": "SupplierMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/supplier.example.com/users/Admin@supplier.example.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.supplier.example.com",
        "peer1.supplier.example.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/supplier.example.com/users/Admin@supplier.example.com/msp/signcerts/Admin@supplier.example.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.supplier.example.com": {
      "url": "grpcs://peer0.supplier.example.com:7041",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/supplier.example.com/msp/tlscacerts/tlsca.supplier.example.com-cert.pem"
      }
    },
    "peer1.supplier.example.com": {
      "url": "grpcs://peer1.supplier.example.com:7042",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/supplier.example.com/msp/tlscacerts/tlsca.supplier.example.com-cert.pem"
      }
    }
  },
  "channels": {
    "supply": {
      "peers": {}
    }
  }
}
